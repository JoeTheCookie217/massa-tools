		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.29.0",
		"highlight.js": "^11.8.0",
		"postcss": "^8.4.27",
		"prettier": "^2.8.8",
